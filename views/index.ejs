            <div class="reviews">
                <form action="/review" method="post" class="col-xl-5 col-lg-5 col-md-5 col-sm-12" onsubmit="reviewFunction()"  data-aos="fade-right">
                    <h2 id="reviewHeaderContentLeft">write your review</h2>
                    <textarea type="text" name="message" id="message" placeholder="Enter Your Review..." ></textarea>
                    <div class="col-12">
                        <button type="submit" class="btn btn-primary" id="sendSubmit">Submit</button>
                    </div>
                </form>
                <div class="col-xl-5 col-lg-5 col-md-5 col-sm-12 comments"  data-aos="fade-left">
                    <h2 id="reviewHeaderContentRight">Customers Reviews</h2>
                    <div class="col-12">
                        <% for(let comment of comments){ %>
                            <p>
                                <i class="fa-solid fa-user"></i>
                                <span><%= comment.text  %> </span>
                            </p>
                        <% } %>
                    </div>
