        <!--Section =========   book start-->
        <section class="container-fluid book" id="book">
            <h1 class="title" id="bookHeader">
                Book
            </h1>
            <hr class="small">
            <form method="post" action="/book" onsubmit="alertFunction()"  data-aos="zoom-in">
                <div class="form-group col-xl-5 col-lg-5 col-md-5 col-sm-10">
                    <label for="phone" id="phoneNumber">Your Number</label>
                    <input type="text" class="form-control" id="phone" name="phone" aria-describedby="emailHelp">
                  </div>
                <div class="form-group col-xl-5 col-lg-5 col-md-5 col-sm-10">
                    <label for="exampleInputEmail1" id="email">Email address</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" name="email" aria-describedby="emailHelp">
                  </div>
                  <div class="form-group col-xl-5 col-lg-5 col-md-5 col-sm-10">
                    <label for="rooms" id="roomCount">number of rooms</label>
                    <input type="number" class="form-control" id="rooms" min="1" value="1" name="rooms">
                  </div>
                  <div class="form-group col-xl-5 col-lg-5 col-md-5 col-sm-10">
                    <label for="persons" id="personCount">number of people</label>
                    <input type="number" class="form-control" id="persons" min="1" value="1" name="persons">
                  </div>
                  <div class="col-12">
                    <button type="submit" class="btn btn-primary" id="bookSubmit">Submit</button>
                  </div>
            </form>
            <div class="contact col-12">
                <a href="https://api.whatsapp.com/send/?phone=966506777339" style="text-decoration:none !important ;" >
                    <div class="parent-social"  data-aos="fade-right">
